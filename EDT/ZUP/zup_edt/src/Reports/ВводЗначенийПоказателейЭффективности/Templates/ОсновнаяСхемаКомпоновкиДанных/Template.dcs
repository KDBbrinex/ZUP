			<availableValue>
				<value xsi:type="dcscor:DesignTimeValue">Перечисление.Периодичность.Квартал</value>
				<presentation xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Квартал</v8:content>
					</v8:item>
