<?xml version="1.0" encoding="UTF-8"?>
<DataCompositionSchema xmlns="http://v8.1c.ru/8.1/data-composition-system/schema" xmlns:dcscom="http://v8.1c.ru/8.1/data-composition-system/common" xmlns:dcscor="http://v8.1c.ru/8.1/data-composition-system/core" xmlns:dcsset="http://v8.1c.ru/8.1/data-composition-system/settings" xmlns:v8="http://v8.1c.ru/8.1/data/core" xmlns:v8ui="http://v8.1c.ru/8.1/data/ui" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<dataSource>
		<name>ИсточникДанных1</name>
		<dataSourceType>Local</dataSourceType>
	</dataSource>
	<dataSet xsi:type="DataSetQuery">
		<name>НаборДанных1</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>Мероприятие</dataPath>
			<field>Мероприятие</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Подразделение</dataPath>
			<field>ПодразделениеОперативныйПлан</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Подразделение</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>УчебныхЧасовПланОбучения</dataPath>
			<field>УчебныхЧасовПланОбучения</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Учебных часов план обучения</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаОкончания</dataPath>
			<field>ДатаОкончания</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СтруктурноеПодразделение</dataPath>
			<field>ПодразделениеПланОбучения</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Структурное подразделение</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСотрудниковПланОбучения</dataPath>
			<field>КоличествоСотрудниковПланОбучения</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество сотрудников план обучения</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>УчебныхЧасовОперативныйПлан</dataPath>
			<field>УчебныхЧасовОперативныйПлан</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Учебных часов оперативный план</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаРасходовПланОбучения</dataPath>
			<field>СуммаРасходовПланОбучения</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма расходов план обучения</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДатаНачала</dataPath>
			<field>ДатаНачала</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаРасходовОперативныйПлан</dataPath>
			<field>СуммаРасходовОперативныйПлан</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма расходов оперативный план</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСотрудниковОперативныйПлан</dataPath>
			<field>КоличествоСотрудниковОперативныйПлан</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество сотрудников оперативный план</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаРасходовФакт</dataPath>
			<field>СуммаРасходовФакт</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма расходов факт</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>УчебныхЧасовФакт</dataPath>
			<field>УчебныхЧасовФакт</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Учебных часов факт</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСотрудниковФакт</dataPath>
			<field>КоличествоСотрудниковФакт</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Количество сотрудников факт</v8:content>
				</v8:item>
			</title>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Подразделение</dataPath>
			<field>Подразделение</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаРасходов</dataPath>
			<field>СуммаРасходов</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоСотрудников</dataPath>
			<field>КоличествоСотрудников</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КоличествоУчебныхЧасов</dataPath>
			<field>КоличествоУчебныхЧасов</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ПланОбученияПоМероприятиям.Подразделение КАК ПодразделениеПланОбучения,
	ПланОбученияПоМероприятиям.Мероприятие КАК Мероприятие,
	ПланОбученияПоМероприятиям.ДатаНачала КАК ДатаНачала,
	ПланОбученияПоМероприятиям.ДатаОкончания КАК ДатаОкончания,
	ПланОбученияПоМероприятиям.КоличествоСотрудников КАК КоличествоСотрудниковПланОбучения,
	ПланОбученияПоМероприятиям.КоличествоУчебныхЧасов КАК УчебныхЧасовПланОбучения,
	ПланОбученияПоМероприятиям.СуммаРасходов КАК СуммаРасходовПланОбучения
ПОМЕСТИТЬ ВТПланОбучения
{ВЫБРАТЬ
	ПодразделениеПланОбучения.*,
	Мероприятие.*,
	ДатаНачала,
	ДатаОкончания,
	КоличествоСотрудниковПланОбучения,
	УчебныхЧасовПланОбучения,
	СуммаРасходовПланОбучения}
ИЗ
	РегистрСведений.ПланОбученияПоМероприятиям КАК ПланОбученияПоМероприятиям
ГДЕ
	ПланОбученияПоМероприятиям.ДатаНачала МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
{ГДЕ
	ПланОбученияПоМероприятиям.Подразделение.*,
	ПланОбученияПоМероприятиям.Мероприятие.*,
	ПланОбученияПоМероприятиям.ДатаНачала,
	ПланОбученияПоМероприятиям.ДатаОкончания,
	ПланОбученияПоМероприятиям.КоличествоСотрудников,
	ПланОбученияПоМероприятиям.КоличествоУчебныхЧасов,
	ПланОбученияПоМероприятиям.СуммаРасходов}

ИНДЕКСИРОВАТЬ ПО
	ПодразделениеПланОбучения,
	Мероприятие
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ПланОбучения.ПодразделениеПланОбучения КАК ПодразделениеПланОбучения,
	ПланОбучения.Мероприятие КАК Мероприятие,
	ПодчиненностьСтруктурныхЕдиниц.СтруктурнаяЕдиница КАК Подразделение
ПОМЕСТИТЬ ВТАнализируемыеПодразделения
ИЗ
	ВТПланОбучения КАК ПланОбучения
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.ПодчиненностьСтруктурныхЕдиниц КАК ПодчиненностьСтруктурныхЕдиниц
		ПО ПланОбучения.ПодразделениеПланОбучения = ПодчиненностьСтруктурныхЕдиниц.ВышестоящаяСтруктурнаяЕдиница
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТАнализируемыеПодразделения.ПодразделениеПланОбучения КАК ПодразделениеПланОбучения,
	ПланОбученияСотрудников.Подразделение КАК ПодразделениеОперативныйПлан,
	ПланОбученияСотрудников.Мероприятие КАК Мероприятие,
	ПланОбученияСотрудников.ДатаНачала КАК ДатаНачала,
	ПланОбученияСотрудников.ДатаОкончания КАК ДатаОкончания,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ПланОбученияСотрудников.ФизическоеЛицо) КАК КоличествоСотрудниковОперативныйПлан,
	СУММА(ПланОбученияСотрудников.КоличествоУчебныхЧасов) КАК УчебныхЧасовОперативныйПлан,
	СУММА(ПланОбученияСотрудников.СуммаРасходов) КАК СуммаРасходовОперативныйПлан
ПОМЕСТИТЬ ВТОперативныйПлан
ИЗ
	РегистрСведений.ПланОбученияСотрудников КАК ПланОбученияСотрудников
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТАнализируемыеПодразделения КАК ВТАнализируемыеПодразделения
		ПО ПланОбученияСотрудников.Подразделение = ВТАнализируемыеПодразделения.Подразделение
			И ПланОбученияСотрудников.Мероприятие = ВТАнализируемыеПодразделения.Мероприятие
ГДЕ
	ПланОбученияСотрудников.ДатаНачала МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания

СГРУППИРОВАТЬ ПО
	ПланОбученияСотрудников.Подразделение,
	ПланОбученияСотрудников.Мероприятие,
	ПланОбученияСотрудников.ДатаНачала,
	ПланОбученияСотрудников.ДатаОкончания,
	ВТАнализируемыеПодразделения.ПодразделениеПланОбучения
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТПланОбучения.ПодразделениеПланОбучения КАК ПодразделениеПланОбучения,
	ВТПланОбучения.Мероприятие КАК Мероприятие,
	ВТПланОбучения.ДатаНачала КАК ДатаНачала,
	ВТПланОбучения.ДатаОкончания КАК ДатаОкончания,
	ВТПланОбучения.КоличествоСотрудниковПланОбучения КАК КоличествоСотрудниковПланОбучения,
	ВТПланОбучения.УчебныхЧасовПланОбучения КАК УчебныхЧасовПланОбучения,
	ВТПланОбучения.СуммаРасходовПланОбучения КАК СуммаРасходовПланОбучения,
	ВТОперативныйПлан.ПодразделениеОперативныйПлан КАК ПодразделениеОперативныйПлан,
	ВТОперативныйПлан.КоличествоСотрудниковОперативныйПлан КАК КоличествоСотрудниковОперативныйПлан,
	ВТОперативныйПлан.УчебныхЧасовОперативныйПлан КАК УчебныхЧасовОперативныйПлан,
	ВТОперативныйПлан.СуммаРасходовОперативныйПлан КАК СуммаРасходовОперативныйПлан
ПОМЕСТИТЬ ВТПлан
ИЗ
	ВТПланОбучения КАК ВТПланОбучения
		ЛЕВОЕ СОЕДИНЕНИЕ ВТОперативныйПлан КАК ВТОперативныйПлан
		ПО ВТПланОбучения.ПодразделениеПланОбучения = ВТОперативныйПлан.ПодразделениеПланОбучения
			И ВТПланОбучения.Мероприятие = ВТОперативныйПлан.Мероприятие
			И ВТПланОбучения.ДатаНачала = ВТОперативныйПлан.ДатаНачала
			И ВТПланОбучения.ДатаОкончания = ВТОперативныйПлан.ДатаОкончания
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОбучениеСотрудников.Мероприятие КАК Мероприятие,
	ОбучениеСотрудников.ФизическоеЛицо КАК ФизическоеЛицо,
	ОбучениеСотрудников.КоличествоУчебныхЧасов КАК КоличествоУчебныхЧасов,
	ОбучениеСотрудников.СуммаРасходов КАК СуммаРасходов,
	ОбучениеСотрудников.ДатаНачала КАК ДатаНачала,
	ОбучениеСотрудников.Сотрудник КАК Сотрудник
ПОМЕСТИТЬ ВТОбучениеСотрудников
ИЗ
	РегистрСведений.ОбучениеСотрудников КАК ОбучениеСотрудников
ГДЕ
	ОбучениеСотрудников.ДатаНачала МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ОбучениеСотрудников.Сотрудник КАК Сотрудник,
	ОбучениеСотрудников.ДатаНачала КАК Период
ПОМЕСТИТЬ ВТСотрудникиПериоды
ИЗ
	ВТОбучениеСотрудников КАК ОбучениеСотрудников
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗНАЧЕНИЕ(Справочник.Сотрудники.ПустаяСсылка) КАК Сотрудник,
	ЗНАЧЕНИЕ(Справочник.ФизическиеЛица.ПустаяСсылка) КАК ФизическоеЛицо,
	ЗНАЧЕНИЕ(Справочник.ШтатноеРасписание.ПустаяСсылка) КАК ДолжностьПоШтатномуРасписанию,
	ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка) КАК МестоВСтруктуреПредприятия,
	ДАТАВРЕМЯ(1, 1, 1) КАК Период
ПОМЕСТИТЬ Представления_КадровыеДанныеСотрудников
ИЗ
	ВТСотрудникиПериоды КАК ВТСотрудникиПериоды
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ ПЕРВЫЕ 0
	Представления_КадровыеДанныеСотрудников.Сотрудник КАК Сотрудник,
	Представления_КадровыеДанныеСотрудников.ДолжностьПоШтатномуРасписанию КАК ДолжностьПоШтатномуРасписанию
ИЗ
	Представления_КадровыеДанныеСотрудников КАК Представления_КадровыеДанныеСотрудников
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	КадровыеДанные.ФизическоеЛицо КАК ФизическоеЛицо,
	КадровыеДанные.Период КАК Период,
	МАКСИМУМ(КадровыеДанные.МестоВСтруктуреПредприятия) КАК МестоВСтруктуреПредприятия
ПОМЕСТИТЬ ВТПодразделенияФизическихЛиц
ИЗ
	Представления_КадровыеДанныеСотрудников КАК КадровыеДанные

СГРУППИРОВАТЬ ПО
	КадровыеДанные.ФизическоеЛицо,
	КадровыеДанные.Период
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТАнализируемыеПодразделения.ПодразделениеПланОбучения КАК ПодразделениеПланОбучения,
	ПодразделенияФизическихЛиц.МестоВСтруктуреПредприятия КАК ПодразделениеФакт,
	ОбучениеСотрудников.Мероприятие КАК Мероприятие,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ ОбучениеСотрудников.ФизическоеЛицо) КАК КоличествоСотрудниковФакт,
	СУММА(ОбучениеСотрудников.КоличествоУчебныхЧасов) КАК УчебныхЧасовФакт,
	СУММА(ОбучениеСотрудников.СуммаРасходов) КАК СуммаРасходовФакт
ПОМЕСТИТЬ ВТФакт
ИЗ
	ВТОбучениеСотрудников КАК ОбучениеСотрудников
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТПодразделенияФизическихЛиц КАК ПодразделенияФизическихЛиц
		ПО ОбучениеСотрудников.ФизическоеЛицо = ПодразделенияФизическихЛиц.ФизическоеЛицо
			И ОбучениеСотрудников.ДатаНачала = ПодразделенияФизическихЛиц.Период
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВТАнализируемыеПодразделения КАК ВТАнализируемыеПодразделения
		ПО (ПодразделенияФизическихЛиц.МестоВСтруктуреПредприятия = ВТАнализируемыеПодразделения.Подразделение)
			И ОбучениеСотрудников.Мероприятие = ВТАнализируемыеПодразделения.Мероприятие
ГДЕ
	ОбучениеСотрудников.ДатаНачала МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания

СГРУППИРОВАТЬ ПО
	ОбучениеСотрудников.Мероприятие,
	ПодразделенияФизическихЛиц.МестоВСтруктуреПредприятия,
	ВТАнализируемыеПодразделения.ПодразделениеПланОбучения
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТПлан.ПодразделениеПланОбучения КАК ПодразделениеПланОбучения,
	ВТПлан.Мероприятие КАК Мероприятие,
	ВТПлан.ДатаНачала КАК ДатаНачала,
	ВТПлан.ДатаОкончания КАК ДатаОкончания,
	ВТПлан.КоличествоСотрудниковПланОбучения КАК КоличествоСотрудниковПланОбучения,
	ВТПлан.УчебныхЧасовПланОбучения КАК УчебныхЧасовПланОбучения,
	ВТПлан.СуммаРасходовПланОбучения КАК СуммаРасходовПланОбучения,
	ВТПлан.ПодразделениеОперативныйПлан КАК ПодразделениеОперативныйПлан,
	ВТПлан.КоличествоСотрудниковОперативныйПлан КАК КоличествоСотрудниковОперативныйПлан,
